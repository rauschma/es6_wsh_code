{
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015-node5": "^1.2.0",
    "co": "^4.6.0",
    "denodeify": "^1.2.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "//": "Things only needed by tests (mocha needs babel-register)",
  "devDependencies": {
    "babel-register": "^6.7.2",
    "eslint": "^2.5.3",
    "http-server": "^0.9.0",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "b": "babel-node",
    "test": "mocha --ui qunit --compilers js:babel-register",
    "http-server": "http-server data/",
    "lint": "eslint"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "rules": {
      "comma-dangle": [1, "always-multiline"],
      "no-console": 0,
      "prefer-const": 1,
      "no-var": 1,
      "no-const-assign": 2,
      "semi": [1, "always"]
    },
    "globals": {
      "console": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rauschma/es6_wsh_code"
  },
  "author": "Axel Rauschmayer",
  "license": "MIT"
}
